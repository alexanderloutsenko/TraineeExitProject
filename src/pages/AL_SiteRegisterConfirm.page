<apex:page showHeader="false" title="Registration Confirmation">
    <apex:define name="body">  
      <center>
        <apex:panelGrid bgcolor="white" columns="1" style="align: center;"> 
          <br/>
          <br/>
          <apex:panelGrid width="758" cellpadding="0" cellspacing="0" bgcolor="lightblue" columns="1" style="topPanel"> 
            <br/>
            <apex:outputPanel layout="block" styleClass="topPanelContainer">
              <apex:panelGrid width="758" cellpadding="0" cellspacing="0" bgcolor="white" >
                <apex:panelGroup >
                  <br/>
                  <apex:outputText value="Thank you for registering." style="font-size: 120%" />
                  <br/>
                  <apex:outputText value="If your position is a region superviser please contact support team to get appropriate permissions." style="font-size: 120%"/>
                  <br/>
                  <br/>
                  <apex:outputLink value="{!$Page.AL_SiteLogin}">Go to Login Page</apex:outputLink>
                  <br/>
                </apex:panelGroup>
              </apex:panelGrid> 
            </apex:outputPanel>
          </apex:panelGrid> 
       </apex:panelGrid>
      </center>
      <br/>
    </apex:define>
</apex:page>
                   